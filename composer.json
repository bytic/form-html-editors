{
  "name": "bytic/form-html-editors",
  "type": "library",
  "description": "WYSIWYG editor for bytic forms ",
  "homepage": "https://github.com/bytic/form-html-editors",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Form\\HtmlEditors\\": "src"
    },
    "classmap": [
      "src/Form"
    ],
    "files": [
      "src/functions/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Form\\HtmlEditors\\Tests\\": "tests/src",
      "ByTIC\\Form\\HtmlEditors\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.2|^8.0",
    "ext-json": "*",
    "bytic/config": "^1.0"
  },
  "require-dev": {
    "bytic/form": "^1.0",
    "ezyang/htmlpurifier": "^4.0",
    "tinymce/tinymce": "^4.8|^5.0",
    "bytic/phpqatools": "^1.0",
    "mockery/mockery": "^1.2"
  }
}
